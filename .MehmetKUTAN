
#xcowsay Hellüü
#cmatrix
fortune | cowsay 


export TURTLEBOT3_MODEL=waffle
export TURTLEBOT3_MODEL=waffle_pi
export TURTLEBOT3_MODEL=burger
export ROS_DOMAIN_ID=32

alias bashrc='gedit ~/.bashrc; echo \"gedit .bashrc\"'

alias Mehmetrc='gedit ~/MehmetKUTAN/.MehmetKUTAN; echo \"gedit .MehmetKUTAN\"'

alias bashrcsource='source ~/.bashrc'

alias gazeboSource='source /usr/share/gazebo-11/setup.sh '

alias update='sudo apt autoremove && sudo apt autoclean && sudo apt autopurge && sudo apt update && sudo apt upgrade; echo \"sudo apt update and upgrade!\" '


alias foxy='source /opt/ros/foxy/setup.bash; echo \"Foxy is Activated!\"'
alias humble='source /opt/ros/humble/setup.bash; echo \"Humble is Activated!\"'

alias ros2colcon='humble; colcon build;ros2source'

alias ros2source='humble;source install/setup.bash'

alias ros2remove="rm -rf build/ install/ log/"

alias ros2ws="cd ~/Documents/ros2_ws/"

alias Groot="humble;cd ~/Documents/ros2_ws/src/Groot/build; ./Groot"


alias amr_mini.launch.py='cd ~/Documents/ros2_ws/;ros2colcon; ros2source; ros2 launch amr_mini_bringup amr_mini.launch.py; echo \"ros2 launch amr_mini.launch.py\"'

alias rosbridge="humble; echo \'Humble is Activated!\';ros2 launch rosbridge_server rosbridge_websocket_launch.xml address:=192.168.108.89 port:=9090"

alias npm_start="cd ~/Documents/web_dev_project/my_project; npm start"

source /usr/share/gazebo-11/setup.sh
